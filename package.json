{
  "name": "example-app",
  "version": "0.1.0",
  "description": "A react app using pouchdb built with parcel in typescript",
  "author": "Stan Lemon <stanlemon@users.noreply.github.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:stanlemon/parcel-app.git"
  },
  "main": "./src/index.tsx",
  "scripts": {
    "start": "parcel --port process.env.PORT --out-dir ./out/ index.html",
    "test": "jest",
    "build": "tsc --build tsconfig.json ./",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "jest": {
    "roots": [
      "./src/"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "babel-jest"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@stanlemon/react-couchdb-authentication": "^0.2.3",
    "@stanlemon/react-form": "^0.2.2",
    "@stanlemon/react-pouchdb": "^0.1.5",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/uuid": "^7.0.2",
    "bulma": "^0.8.1",
    "pouchdb": "^7.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.1.4",
    "@types/parcel-env": "0.0.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.2.3",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.2",
    "react-hot-loader": "^4.12.20",
    "typescript": "^3.8.3",
    "wait-for-expect": "^3.0.2"
  }
}
