{
  "name": "example-app",
  "version": "0.1.0",
  "description": "A react app using pouchdb built with parcel in typescript",
  "author": "Stan Lemon <stanlemon@users.noreply.github.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:stanlemon/parcel-app.git"
  },
  "main": "./src/index.tsx",
  "scripts": {
    "start": "parcel --port process.env.PORT --out-dir ./out/ index.html",
    "test": "jest",
    "build": "tsc --build tsconfig.json ./",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/"
  },
  "browserslist": [
    "last 1 year, not dead",
    "edge > 18",
    "not ie 11",
    "not ie_mob 11",
    "not samsung < 1000",
    "not opera < 1000",
    "not kaios < 1000",
    "not android < 1000",
    "not and_qq < 1000"
  ],
  "jest": {
    "roots": [
      "./src/"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "babel-jest"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@stanlemon/react-couchdb-authentication": "^0.2.8",
    "@stanlemon/react-pouchdb": "^0.1.12",
    "@types/pouchdb": "^6.4.0",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.0.0",
    "bulma": "^0.9.0",
    "pouchdb": "^7.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.3",
    "@types/parcel-env": "0.0.0",
    "@types/react": "^16.9.41",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "jest": "^26.1.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.21",
    "typescript": "^3.9.6",
    "wait-for-expect": "^3.0.2"
  }
}
