{
  "name": "example-app",
  "version": "0.1.0",
  "description": "A react app using pouchdb built with parcel in typescript",
  "author": "Stan Lemon <stanlemon@users.noreply.github.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:stanlemon/parcel-app.git"
  },
  "main": "./src/index.tsx",
  "scripts": {
    "start": "parcel --port process.env.PORT --out-dir ./out/ index.html",
    "test": "jest",
    "build": "tsc --build tsconfig.json ./",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/"
  },
  "browserslist": [
    "last 1 year, not dead",
    "edge > 18",
    "not ie 11",
    "not ie_mob 11",
    "not samsung < 1000",
    "not opera < 1000",
    "not kaios < 1000",
    "not android < 1000",
    "not and_qq < 1000"
  ],
  "jest": {
    "roots": [
      "./src/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "^typeface-(.*)": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@stanlemon/react-couchdb-authentication": "^0.4.1",
    "@stanlemon/react-pouchdb": "^0.1.15",
    "@types/pouchdb": "^6.4.0",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.0",
    "bulma": "^0.9.0",
    "date-fns": "^2.15.0",
    "pouchdb": "^7.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.0",
    "parcel-bundler": "^1.12.4",
    "pouchdb-adapter-memory": "^7.2.2",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.21",
    "typescript": "^3.9.7"
  }
}
